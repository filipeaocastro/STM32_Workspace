#ifndef INC_LED_RGB_H_
#define INC_LED_RGB_H_

#ifdef __cplusplus
extern "C" {
#endif

#include "stm32f1xx_hal.h"

#define AZUL 	 0x1
#define VERDE 	 0x2
#define CIANO 	 0x3
#define VERMELHO 0x4
#define ROSA 	 0x5
#define AMARELO  0x6
#define BRANCO 	 0x7

void ligar(uint16_t cor);
void apagar(void);



#ifdef __cplusplus
}
#endif

#endif /* INC_LED_RGB_H_ */
